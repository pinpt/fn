{
  "name": "@pinpt/fn",
  "version": "0.0.7",
  "description": "A tiny micro framework for the Serverless framework for building fast Î» functions",
  "main": "dist/index.js",
  "typings": "dist/@types/index.d.ts",
  "types": "dist/@types/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pinpt/fn.git"
  },
  "keywords": [
    "serverless",
    "lambda"
  ],
  "author": "Pinpoint",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pinpt/fn/issues"
  },
  "homepage": "https://github.com/pinpt/fn#readme",
  "dependencies": {
    "binary-case": "^1.1.4",
    "cookie": "^0.3.1",
    "debug": "^4.1.0",
    "jsonwebtoken": "^8.3.0",
    "koa-route": "^3.2.0",
    "methods": "^1.1.2",
    "mysql": "^2.16.0",
    "source-map-support": "^0.5.0",
    "xxhashjs": "^0.2.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.1",
    "@types/cookie": "^0.3.1",
    "@types/debug": "^0.0.31",
    "@types/jest": "^23.3.1",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/koa-route": "^3.2.4",
    "@types/methods": "^1.1.0",
    "@types/mysql": "^2.15.5",
    "@types/node": "^8.0.57",
    "@types/xxhashjs": "^0.1.1",
    "aws-lambda": "^0.1.2",
    "aws-sdk": "^2.294.0",
    "colors": "^1.3.2",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.2",
    "lodash.camelcase": "^4.3.0",
    "path-to-regexp": "^2.4.0",
    "prettier": "^1.14.2",
    "prompt": "^1.0.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.2.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^7.1.0",
    "typedoc": "^0.12.0",
    "typescript": "^2.8.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs",
    "test": "FN_COOKIE_SECRET=abcd FN_ENC_KEY=abcd jest",
    "test:watch": "jest --watch",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
    "precommit": "lint-staged",
    "prepare": "npm run build"
  }
}
